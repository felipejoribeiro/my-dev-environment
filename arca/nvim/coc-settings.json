      "interface": " 蘒",
      "struct": "  ",
      "event": "  ",
      "operator": "  ",
      "module": "  ",
      "property": "  ",
      "enum": " 練",
      "reference": "  ",
      "keyword": "  ",
      "file": "  ",
      "folder": "  ",
      "color": "  ",
      "unit": " 塞 ",
      "snippet": "  ",
      "text": "  ",
      "constructor": "  ",
      "value": "  ",
      "enumMember": "  ",
      "default":"  "
    },
    "diagnostic.warningSign": "⚠",
